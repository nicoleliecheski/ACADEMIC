#include <stdio.h>
#include <math.h>

int main() {
    int horas, min, seg; //declaração das variáveis
    //este programa foi feito pela aluna: Nicole Liecheski
    //na data 02/05/2023

    //Escreva o valor das horas, minutos e segundos.

    scanf("\n%d\n%d\n%d", &horas, &min, &seg);
    printf("O TEMPO EM SEGUNDOS E = %d\n", (horas*3600)+(min*60)+seg);

return 0;
} 
