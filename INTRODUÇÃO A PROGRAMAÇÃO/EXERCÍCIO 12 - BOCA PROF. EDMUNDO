#include <stdio.h>

int main() {
    float pf, x, y; //declaração das variáveis
    //este programa foi feito pela aluna: Nicole Liecheski
    //na data 02/05/2023

    //Escreva o valor do preço de fábrica do carro, do percentual do distribuidor e do percentual de impostos.

    scanf("\n%f\n%f\n%f", &pf, &x, &y);
    printf("O VALOR DO CARRO E = %.2f\n", pf+(pf*(x+y)/100));

return 0;
}
