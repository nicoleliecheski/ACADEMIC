#include <stdio.h>

int main() {
    float s, watts; //declaração das variáveis
    //este progama foi feito pela aluna: Nicole Liecheski
    //na data 27/04/2022

    //Escreva o valor do salário mínimo e a quantidade de kW gasta pela residência.

    scanf("%f %f", &s, &watts);
    printf("Custo por kW: R$ %.2f\nCusto do consumo: R$ %.2f\nCusto com desconto: R$ %.2f\n", 0.007*s, 0.007*s*watts, 0.007*s*watts*0.9);
return 0;
} 
